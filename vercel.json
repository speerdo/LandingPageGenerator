{
  "version": 2,
  "builds": [
    {
      "src": "src/api/scrape.ts",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/scrape",
      "dest": "/src/api/scrape.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 